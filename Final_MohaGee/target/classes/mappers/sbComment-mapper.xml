<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC
"-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sbComment-mapper">

	<select id="sbCommentCount" resultType="int">
        SELECT
            COUNT(*)
        FROM COMMENT
    </select>
    
    <select id="sbCommentList" resultType="int">
        SELECT
            *
        FROM BCOMMENT
    </select>
    
    
    <insert id="sbCommentInsert" parameterType="int">
        INSERT INTO
        BCOMMENT VALUES() 
    </insert>
    
    <update id="sbCommentUpdate" parameterType="int">
       
    </update>
    
    <delete id="sbCommentDelete" parameterType="int">
        DELETE FROM COMMENT WHERE BCNO = #{bcNo}
    </delete>

	
</mapper> 











